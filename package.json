{
  "name": "hekaheka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "hardhat compile",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:contracts": "hardhat test",
    "deploy:contracts": "hardhat run contracts/scripts/deploy.ts --network baseSepolia",
    "test:mint": "ts-node --project tsconfig.node.json tests/test-mint.ts",
    "test:marketplace": "ts-node --project tsconfig.node.json tests/test-marketplace.ts",
    "test:api": "mocha --require ts-node/register tests/api/*.test.ts --exit",
    "check:balance": "ts-node --project tsconfig.node.json tests/check-balance.ts",
    "test:ipfs": "ts-node --project tsconfig.node.json tests/test-ipfs.ts",
    "postbuild": "cp -r typechain-types ./app/typechain-types && cp -r artifacts ./app/contracts/artifacts"
  },
  "dependencies": {
    "@coinbase/agentkit": "^0.7.1",
